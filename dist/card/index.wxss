.bs-card-container {
  display: flex;
  color: #108ee9;
  width: 100%;
  box-shadow: 1px 1px 1px 1px rgba(89, 101, 101, 0.53);
  background-color: #ffffff;
}
.bs-card-left {
  height: 100px;
  width: 100px;
}
.bs-card-right {
  height: 90px;
  padding: 5px 10px 5px 10px;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: flex-start;
}
.bs-card-title {
  color: black;
  font-size: 16px;
}
.bs-card-descp {
  font-size: 12px;
  margin-top: -16px;
}
.bs-card-remark {
  font-size: 16px;
  color: red;
}
